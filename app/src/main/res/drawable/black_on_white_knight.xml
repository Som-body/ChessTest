<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
            <solid android:color="@color/white" />
        </shape>
    </item>

    <item
        android:right="15dp"
        android:left="15dp"
        android:top="75dp"
        android:bottom="5dp">
        <rotate
            android:fromDegrees="0">
            <shape android:shape="rectangle">
                <solid android:color="@color/black" />
            </shape>
        </rotate>
    </item>

    <item
        android:right="32dp"
        android:left="32dp"
        android:top="50dp"
        android:bottom="14dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/black" />
            </shape>
        </rotate>
    </item>

    <item
        android:right="52dp"
        android:left="28dp"
        android:top="15dp"
        android:bottom="40dp">
        <rotate
            android:fromDegrees="0">
            <shape android:shape="oval">
                <solid android:color="@color/black" />
            </shape>
        </rotate>
    </item>

    <item
        android:right="50dp"
        android:left="35dp"
        android:top="16dp"
        android:bottom="40dp">
        <rotate
            android:fromDegrees="0">
            <shape android:shape="rectangle">
                <solid android:color="@color/black" />
            </shape>
        </rotate>
    </item>

    <item
        android:right="25dp"
        android:left="32dp"
        android:top="15dp"
        android:bottom="60dp">
        <rotate
            android:fromDegrees="25">
            <shape android:shape="rectangle">
                <solid android:color="@color/black" />
            </shape>
        </rotate>
    </item>

</layer-list>